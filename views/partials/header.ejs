<header>
	<h1><%=header%></h1>
	<nav class="main-nav">
		<div class="mainNav">
			<a href="/earfull">Home</a> 
		</div>
		<div class="authNav">
			<a href="/earfull/auth/register">Create New Account</a>  
			<% if (loggedIn) { -%>
				<form action="/earfull/auth/logout" method="POST">
					<button>Logout</button>
				</form>
			<% } else { -%>
					<br />
					<a href="/earfull/auth/login">Log In</a>
			<% } -%>
		</div>
		<p><%=message%> &nbsp;</p>
	</nav>
</header>